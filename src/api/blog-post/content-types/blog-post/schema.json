{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Intro": {
      "type": "text"
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "shared.callout",
        "shared.50-50-image-richtext",
        "shared.richtext",
        "shared.image"
      ]
    },
    "Published": {
      "type": "boolean"
    }
  }
}
